<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="360.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.newyeargreetings.Controller">
    <padding>

    </padding>
   <CheckBox fx:id = "regularCustomer" mnemonicParsing="false"  text="Regular customer" />
    <HBox alignment="CENTER">
      <ComboBox fx:id="manufacturersBox" onAction="#addManufacturers" prefWidth="150.0" />
      <ComboBox fx:id="giftsBox" prefWidth="150.0" />
      <ComboBox fx:id="concertsBox" onAction="#initialize" prefWidth="150.0" />
    </HBox>
    <Label fx:id="costGift" textAlignment="JUSTIFY" />
   <Label fx:id="costConcert" textAlignment="JUSTIFY" />
    <Label fx:id="cost" textAlignment="JUSTIFY" />
</VBox>
